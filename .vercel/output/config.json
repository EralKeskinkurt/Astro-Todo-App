{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_image.astro"
		},
		{
			"src": "^\\/$",
			"dest": "index.astro"
		},
		{
			"src": "^\\/create\\/?$",
			"dest": "create.astro"
		},
		{
			"src": "^\\/update\\/([^/]+?)\\/?$",
			"dest": "_id_.astro"
		},
		{
			"src": "^\\/api\\/controller$",
			"dest": "controller.astro"
		}
	]
}